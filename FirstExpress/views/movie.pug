extends layout

block content
    -const imageAttributes={"src":imageBaseUrl+"w500"+movie.poster_path ,"alt":imageBaseUrl+"w500"+movie.poster_path}
    img&attributes(imageAttributes)#displayimage
    #displayinfo
        h1=movie.title
        h4 Release Date:  #{movie.release_date}
        h4 Rating: #{movie.vote_average}
        h4 Overview:
        p #{movie.overview}

